<?xml version="1.0" encoding="utf-8"?>
<manifest
	package="org.xzc.learnmaterialdrawer"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".CompactStyleActivity"/>
		<activity android:name=".UnderActionBarActivity"/>
		<activity android:name=".LeftAndRightDrawerActivity"/>
		<activity android:name=".UnderStatusBarActivity"/>
		<activity android:name=".MenuActivity"/>
		<activity android:name=".InsetActivity"/>
		<activity android:name=".MiniActivity"/>
		<activity android:name=".AboutActivity"/>
	</application>

</manifest>
